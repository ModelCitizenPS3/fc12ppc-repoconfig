[fedora-updates]
name=Fedora 12 - Updates - ppc64
baseurl=http://archive.fedoraproject.org/pub/archive/fedora/linux/updates/12/ppc64/
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-fedora-12-primary
exclude=fedora-release

[fedora-updates-SRPMS]
name=Fedora 12 SRPMS - Updates - ppc64
baseurl=http://archive.fedoraproject.org/pub/archive/fedora/linux/updates/12/SRPMS/
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-fedora-12-primary
exclude=fedora-release

[fedora-updates-debug]
name=Fedora 12 debug - Updates - ppc64
baseurl=http://archive.fedoraproject.org/pub/archive/fedora/linux/updates/12/ppc64/debug/
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-fedora-12-primary
exclude=fedora-release

