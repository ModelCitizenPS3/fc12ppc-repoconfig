[fedora-updates-testing]
name=Fedora $releasever - Updates - Testing - $basearch
baseurl=http://archive.fedoraproject.org/pub/archive/fedora/linux/updates/testing/$releasever/$basearch/
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-fedora-$releasever-primary
exclude=fedora-release lapack lapack-devel blas blas-devel

[fedora-updates-testing-SRPMS]
name=Fedora $releasever - SRPMs - Updates - Testing - $basearch
baseurl=http://archive.fedoraproject.org/pub/archive/fedora/linux/updates/testing/$releasever/SRPMS/
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-fedora-$releasever-primary
exclude=fedora-release lapack lapack-devel blas blas-devel

[fedora-updates-testing-debug]
name=Fedora $releasever - debug - Updates - Testing - $basearch
baseurl=http://archive.fedoraproject.org/pub/archive/fedora/linux/updates/testing/$releasever/$basearch/debug/
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-fedora-$releasever-primary
exclude=fedora-release lapack lapack-devel blas blas-devel

