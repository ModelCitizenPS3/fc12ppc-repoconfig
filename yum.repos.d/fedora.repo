[fedora]
name=Fedora 12 - ppc
baseurl=http://archive.fedoraproject.org/pub/archive/fedora/linux/releases/12/Everything/ppc/os/
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-fedora-12-primary
exclude=fedora-release

[fedora-SRPMs]
name=Fedora 12 SRPMs - ppc
baseurl=http://archive.fedoraproject.org/pub/archive/fedora/linux/releases/12/Everything/source/SRPMS/
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-fedora-12-primary
exclude=fedora-release

[fedora-debug]
name=Fedora 12 debug - ppc
baseurl=http://archive.fedoraproject.org/pub/archive/fedora/linux/releases/12/Everything/ppc/debug/
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-fedora-12-primary
exclude=fedora-release

